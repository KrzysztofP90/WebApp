<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">


  <servlet>
    <servlet-name>StartServlet</servlet-name>
    <servlet-class>StartServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>StartServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CmsServlet</servlet-name>
    <servlet-class>CmsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CmsServlet</servlet-name>
    <url-pattern>/cms</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>EditDataBaseServlet</servlet-name>
    <servlet-class>EditDataBaseServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EditDataBaseServlet</servlet-name>
    <url-pattern>/edited</url-pattern>
  </servlet-mapping>


</web-app>
